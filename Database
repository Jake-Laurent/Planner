CREATE TABLE users(User_id INT NOT NULL, name VARCHAR(50) NOT NULL, password VARCHAR(50) NOT NULL, birthday VARCHAR(50) NOT NULL, email VARCHAR(50) NOT NULL, PRIMARY KEY (User_id)); 
CREATE TABLE student(Student_id INT NOT NULL, User_id INT NOT NULL, major VARCHAR(50) NOT NULL, minor VARCHAR(50), year INT NOT NULL, PRIMARY KEY (Student_id), FOREIGN KEY (User_id) REFERENCES users (User_id));
CREATE TABLE extra_curricular(EC_id INT NOT NULL, Student_id INT NOT NULL, EC_name VARCHAR(50) NOT NULL, EC_date DATE NOT NULL, EC_time time NOT NULL, PRIMARY KEY (EC_id), FOREIGN KEY (Student_id) REFERENCES student (student_id));
CREATE TABLE personal(To_Do_id INT NOT NULL, User_id INT NOT NULL, Personal_to_do_name VARCHAR(50) NOT NULL, P_date DATE NOT NULL, P_time TIME NOT NULL, PRIMARY KEY (To_do_id), FOREIGN KEY (User_id) REFERENCES users (User_id));
CREATE TABLE work(Work_toDo_id INT NOT NULL, User_id INT NOT NULL, Work_name VARCHAR(50) NOT NULL, W_date DATE NOT NULL, W_time TIME NOT NULL, PRIMARY KEY (Work_ToDo_ID), FOREIGN KEY (User_id) REFERENCES users (User_id));
CREATE TABLE class(Class_id INT NOT NULL, Class_name VARCHAR(50) NOT NULL, Class_date DATE NOT NULL, Class_time TIME NOT NULL, Room VARCHAR(50) NOT NULL, PRIMARY KEY (Class_id), FOREIGN KEY (Student_id) REFERENCES student (Student_id));
CREATE TABLE Assignment(Assignment_id INT NOT NULL, Assignment_name VARCHAR(50) NOT NULL, Due_date DATE NOT NULL, Due_time TIME NOT NULL, PRIMARY KEY (Assignment_id), FOREIGN KEY (Class_id) REFERENCES class (Class_id));
